# gntl environment template
# Copy to .env and edit values for your local deployment.

# Web admin listen ports
GNTL_HTTP_PORT=2027
GNTL_HTTPS_PORT=2026

# FRP defaults
GNTL_INSTANCE_HTTP_PORT=2027
GNTL_INSTANCE_HTTPS_PORT=2026
GNTL_ENABLE_HTTP_ON_CREATE=0

# Mobile runtime options
GNTL_MOBILE_USE_CADDY=0
GNTL_MOBILE_PHP_PORT=2027

# Optional explicit PHP binary override for mobile shells (Termux/iSH)
# Examples: php, php84, /usr/bin/php84
GNTL_PHP_BIN=
